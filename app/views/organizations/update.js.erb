(function() {  

    $('.org-edit .edit_organization').slideToggle();
    $('.org-edit .button').show();
    $("#description").text("<%= j @organization.description %>".replace(/\&#39;/ig,"'"));
    $("#email").text("<%= @organization.email %>");
    $("#street").text("<%= @organization.street %>");
    $("#city").text("<%= @organization.city %>, <%= @organization.state %> <%= @organization.zip %>");
    $("#image").html("<%= j image_tag @organization.image%>");
    
})();